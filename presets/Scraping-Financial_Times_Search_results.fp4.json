{
  "name": "Financial Times: Search results",
  "category": "Scraping",
  "description": "Add search terms as seed nodes and fertch data.",
  "module": "Generic",
  "options": {
    "basepath": "https://www.ft.com",
    "resource": "/search",
    "params": {
      "q": "<Object ID>",
      "sort": "relevance"
    },
    "extension": "",
    "headers": {},
    "verb": "GET",
    "format": "text",
    "filename": "<None>",
    "fileext": "<None>",
    "pages": 1,
    "paging_type": "count",
    "param_paging": "page",
    "offset_start": 1,
    "offset_step": 1,
    "nodedata": "text|css:li.search-results__list-item",
    "objectid": "text|css:a.js-teaser-heading-link|xpath:@href",
    "scope": "",
    "proxy": "",
    "auth_type": "Disable",
    "auth_uri": "",
    "redirect_uri": "",
    "token_uri": "",
    "auth": "disable",
    "auth_tokenname": "",
    "auth_prefix": ""
  },
  "speed": 200,
  "saveheaders": false,
  "timeout": 15,
  "maxsize": 5,
  "columns": [
    "headline=text|css:a|xpath:string()",
    "link=text|xpath://a/@href",
    "text|xpath:string(//p[2])",
    "text|xpath:string(//span[@data-test-id=\"time-and-date:clock\"])",
    "text|xpath:string(//span[@type=\"category\"])"
  ]
}